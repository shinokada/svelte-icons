{
  "name": "flowbite-svelte-starter",
  "version": "0.2.0",
  "description": "SVG icon collection for Svelte",
  "author": {
    "name": "Shinichi Okada",
    "email": "connect@codewithshin.com",
    "url": "https://blog.codewithshin.com"
  },
  "bugs": "https://github.com/shinokada/svelte-icons-website/issues",
  "homepage": "https://github.com/shinokada/svelte-icons-website",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build ",
    "preview": "vite preview",
    "package": "svelte-kit sync && publint",
    "prepublishOnly": "npm run package",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-vercel": "^3.0.3",
    "@sveltejs/kit": "^1.22.4",
    "@sveltejs/package": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-svelte": "^2.32.4",
    "flowbite-svelte": "^0.39.3",
    "flowbite-svelte-icons": "^0.3.3",
    "postcss": "^8.4.27",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.1.2",
    "svelte-check": "^3.4.6",
    "svelte-meta-tags": "^2.8.0",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8"
  },
  "type": "module",
  "dependencies": {
    "@popperjs/core": "2.11.6",
    "classnames": "^2.3.2",
    "flowbite": "^1.8.1"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "flowbite",
    "flowbite-svelte",
    "tailwindcss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shinokada/svelte-icons-website"
  }
}

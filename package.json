{
	"name": "svelte-svg-icon-sets",
	"version": "2.0.0",
	"private": true,
	"description": "SVG icon sets for Svelte 4/5/Runes",
	"license": "MIT",
	"homepage": "https://svelte-icons.codewithshin.com/",
	"author": {
		"name": "Shinichi Okada",
		"email": "connect@codewithshin.com",
		"url": "https://codewithshin.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/shinokada/svelte-icons"
	},
	"bugs": "https://github.com/shinokada/svelte-icons/issues",
	"keywords": [
		"svelte",
		"runes",
		"sveltekit",
		"ui"
	],
	"contributors": [
		{
			"name": "shinokada/svelte-icons",
			"url": "https://github.com/shinokada/svelte-icons"
		}
	],
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.4",
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.26.0",
		"@playwright/test": "^1.52.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.21.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@svitejs/changesets-changelog-github-compact": "^1.2.0",
		"@tailwindcss/vite": "^4.1.6",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.7",
		"eslint": "^9.26.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.6.0",
		"globals": "^16.1.0",
		"highlight.js": "^11.11.1",
		"jsdom": "^26.1.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"runatics": "^0.1.4",
		"runes-meta-tags": "^0.4.3",
		"runes-webkit": "^0.9.58",
		"svelte": "^5.28.6",
		"svelte-5-ui-lib": "^0.12.6",
		"svelte-check": "^4.1.7",
		"svelte-lib-helpers": "^0.4.28",
		"svelte-rune-highlight": "^0.5.15",
		"svelte-shields": "^0.5.1",
		"tailwind-merge": "^3.3.0",
		"tailwindcss": "^4.1.6",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.32.1",
		"vite": "^6.3.5",
		"vitest": "^3.1.3"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}

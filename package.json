{
	"name": "flowbite-svelte-starter",
	"version": "0.2.0",
	"description": "SVG icon collection for Svelte",
	"author": {
		"name": "Shinichi Okada",
		"email": "connect@codewithshin.com",
		"url": "https://blog.codewithshin.com"
	},
	"bugs": "https://github.com/shinokada/svelte-icons-website/issues",
	"homepage": "https://github.com/shinokada/svelte-icons-website",
	"license": "MIT",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/test": "^1.27.1",
		"@popperjs/core": "^2.11.6",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"@typescript-eslint/parser": "^5.41.0",
		"autoprefixer": "10.4.5",
		"classnames": "^2.3.2",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"flowbite": "^1.5.3",
		"flowbite-svelte": "^0.27.11",
		"postcss": "^8.4.18",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.0",
		"svelte": "^3.52.0",
		"svelte-check": "^2.9.2",
		"svelte-heros": "^2.3.5",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.1",
		"tslib": "^2.4.0",
		"typescript": "~4.6.4",
		"vite": "^3.2.1"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"sveltekit",
		"flowbite",
		"flowbite-svelte",
		"tailwindcss"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/shinokada/svelte-icons-website"
	}
}

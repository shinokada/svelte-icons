		"@typescript-eslint/parser": "^5.46.1",
		"autoprefixer": "10.4.5",
		"classnames": "^2.3.2",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"flowbite": "^1.5.5",

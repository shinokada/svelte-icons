{
  "name": "flowbite-svelte-starter",
  "version": "0.2.0",
  "description": "SVG icon collection for Svelte",
  "author": {
    "name": "Shinichi Okada",
    "email": "connect@codewithshin.com",
    "url": "https://blog.codewithshin.com"
  },
  "bugs": "https://github.com/shinokada/svelte-icons-website/issues",
  "homepage": "https://github.com/shinokada/svelte-icons-website",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build ",
    "preview": "vite preview",
    "package": "svelte-kit sync && publint",
    "prepublishOnly": "npm run package",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.0",
    "@sveltejs/package": "^2.2.6",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "flowbite-svelte": "^0.44.22",
    "flowbite-svelte-icons": "^1.0.3",
    "postcss": "^8.4.33",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.3",
    "svelte-meta-tags": "^3.1.0",
    "svelte-preprocess": "^5.1.3",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  },
  "type": "module",
  "dependencies": {
    "@popperjs/core": "2.11.6",
    "classnames": "^2.5.1",
    "flowbite": "^1.8.1"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "flowbite",
    "flowbite-svelte",
    "tailwindcss"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shinokada/svelte-icons-website"
  }
}

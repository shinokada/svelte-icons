{
	"name": "svelte-svg-icon-sets",
	"version": "2.0.0",
	"private": true,
	"description": "SVG icon sets for Svelte 4/5/Runes",
	"license": "MIT",
	"homepage": "https://svelte-icons.codewithshin.com/",
	"author": {
		"name": "Shinichi Okada",
		"email": "connect@codewithshin.com",
		"url": "https://codewithshin.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/shinokada/svelte-icons"
	},
	"bugs": "https://github.com/shinokada/svelte-icons/issues",
	"keywords": [
		"svelte",
		"runes",
		"sveltekit",
		"ui"
	],
	"contributors": [
		{
			"name": "shinokada/svelte-icons",
			"url": "https://github.com/shinokada/svelte-icons"
		}
	],
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test"
	},
	"devDependencies": {
		"@changesets/cli": "^2.29.7",
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.36.0",
		"@playwright/test": "^1.55.0",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.42.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@svitejs/changesets-changelog-github-compact": "^1.2.0",
		"@tailwindcss/vite": "^4.1.13",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/svelte": "^5.2.8",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.3",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.13.13",
		"globals": "^16.4.0",
		"highlight.js": "^11.11.1",
		"jsdom": "^27.0.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"runatics": "^0.1.4",
		"runes-meta-tags": "^0.4.5",
		"runes-webkit": "^0.11.4",
		"svelte": "^5.39.3",
		"svelte-check": "^4.3.1",
		"svelte-lib-helpers": "^0.4.30",
		"svelte-rune-highlight": "^0.7.1",
		"svelte-shields": "^0.5.2",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.0",
		"vite": "^7.1.6",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.4"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
